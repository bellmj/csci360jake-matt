<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="1080.0" prefWidth="1920.0" styleClass="darkList" stylesheets="@IssueTrackingLite.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="system.ui.VotingController">
   <children>
      <VBox alignment="CENTER" layoutX="760.0" layoutY="240.0" prefHeight="600.0" prefWidth="400.0">
         <children>
            <Label fx:id="propositionTitleLabel" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="400.0" text="Title of the thing to be voted on.">
               <font>
                  <Font size="24.0" />
               </font>
               <padding>
                  <Insets bottom="15.0" />
               </padding>
            </Label>
            <ScrollPane fitToHeight="true" fitToWidth="true" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefViewportHeight="500.0" prefViewportWidth="400.0" prefWidth="400.0">
               <content>
                  <Pane styleClass="darkList">
                     <children>
                        <Label fx:id="propositionDescriptionLabel" alignment="TOP_CENTER" prefHeight="498.0" prefWidth="398.0" text="Description of the thing to be voted on." wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </Pane>
               </content>
            </ScrollPane>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="60.0">
               <children>
                  <Button fx:id="propositionPreviousButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="31.0" prefWidth="80.0" text="Previous" />
                  <Button fx:id="propositionCancelButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="31.0" prefWidth="80.0" text="Cancel" />
                  <Button fx:id="propositionNextButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="31.0" prefWidth="80.0" text="Next" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
